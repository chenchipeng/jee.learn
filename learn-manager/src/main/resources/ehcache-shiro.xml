<ehcache xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"	xsi:noNamespaceSchemaLocation="../config/ehcache.xsd" name="shiroEhcache">	<diskStore path="../temp/learn-manager/shiro" />	<defaultCache maxElementsInMemory="10000" eternal="false"		timeToIdleSeconds="120" timeToLiveSeconds="120"		maxElementsOnDisk="10000000" diskExpiryThreadIntervalSeconds="120"		memoryStoreEvictionPolicy="LRU">		<persistence strategy="localTempSwap" />	</defaultCache>		<!-- 系统缓存 -->	<cache name="sysCache" maxEntriesLocalHeap="1000" eternal="true" overflowToDisk="true" statistics="true">	<!-- <cacheEventListenerFactory class="net.sf.ehcache.distribution.RMICacheReplicatorFactory"/> -->    </cache>	</ehcache>